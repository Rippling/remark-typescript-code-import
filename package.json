{
  "name": "remark-typescript-code-import",
  "version": "1.0.0",
  "description": "A remark plugin to import code from typescript files",
  "license": "Apache-2.0",
  "repository": "Rippling/remark-typescript-code-import",
  "bugs": "https://github.com/Rippling/remark-typescript-code-import/issues",
  "author": "Divyansh Kumar <mr.divyanshindore@gmail.com>",
  "keywords": [
    "code import",
    "markdown",
    "mdast",
    "mdx",
    "plugin",
    "remark-plugin",
    "remark",
    "typescript",
    "unified"
  ],
  "engines": {
    "node": ">=16"
  },
  "sideEffects": false,
  "main": "dist/index.js",
  "type": "commonjs",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "prebuild": "npm run clean && npm run lint:fix && tsc",
    "clean": "rimraf dist",
    "format": "prettier --write .",
    "lint": "eslint --ext .ts .",
    "lint:fix": "eslint --ext .ts . --fix",
    "prepublishOnly": "npm run build",
    "test": "npm run build && vitest"
  },
  "dependencies": {
    "mdast": "^3.0.0",
    "typescript": "^5.3.3",
    "unist-util-visit": "^5.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.5",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "mdast-util-directive": "^3.0.0",
    "mdast-util-mdx-jsx": "^3.0.0",
    "prettier": "3.2.4",
    "remark": "^15.0.1",
    "remark-directive": "^3.0.0",
    "rimraf": "^5.0.5",
    "vfile": "^6.0.1",
    "vite": "^5.1.6",
    "vite-plugin-dts": "^3.7.3",
    "vitest": "^1.2.2"
  }
}
